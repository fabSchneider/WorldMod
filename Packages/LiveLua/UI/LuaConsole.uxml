<ui:UXML xmlns:ui="UnityEngine.UIElements" xmlns:uie="UnityEditor.UIElements" xsi="http://www.w3.org/2001/XMLSchema-instance" engine="UnityEngine.UIElements" editor="UnityEditor.UIElements" noNamespaceSchemaLocation="../../UIElementsSchema/UIElements.xsd" editor-extension-mode="False">
    <Style src="project://database/Packages/com.fab.livelua/UI/LuaConsoleStyle.uss?fileID=7433441132597879392&amp;guid=c96f2f6342be4da41b501ff2f0476bd2&amp;type=3#LuaConsoleStyle" />
    <ui:VisualElement picking-mode="Ignore" class="lua-console">
        <ui:Button text="»" display-tooltip-when-elided="true" name="toggle-btn" class="lua-console__toggle-button" />
        <ui:VisualElement picking-mode="Ignore" class="lua-console__container">
            <ui:VisualElement picking-mode="Ignore" class="lua-console__wrapper">
